{
    "name" : "indielab/yii2s3",
    "description" : "Yii 2 S3 Component",
    "type" : "yii2-extension",
    "keywords" : ["s3"],
    "homepage" : "https://github.com/indielab",
    "authors" : [
        {
            "name" : "Basil Suter",
            "email" : "basil@nadar.io",
            "homepage" : "https://github.com/nadar"
        }
    ],
    "require": {
        "aws/aws-sdk-php": "~3.0",
        "yiisoft/yii2" : "~2.0.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "~5.0"
    },
    "autoload" : {
        "psr-4" : {
            "indielab\\yii2s3\\" : "src/",
            "indielab\\yii2s3\\tests\\" : "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
